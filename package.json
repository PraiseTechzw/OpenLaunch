{
  "name": "openlaunch",
  "version": "1.0.0",
  "description": "OpenLaunch - Collaborative Software Development Platform",
  "private": true,
  "workspaces": [
    "website",
    "frontend",
    "backend",
    "ai"
  ],
  "scripts": {
    "dev": "cd website && npm run dev",
    "build": "cd website && npm run build",
    "start": "cd website && npm run start",
    "lint": "cd website && npm run lint",
    "type-check": "cd website && npm run type-check",
    "test": "echo \"Running tests for all workspaces...\" && npm run test --workspaces --if-present",
    "install-all": "npm install && npm install --workspaces",
    "clean": "rm -rf node_modules && rm -rf */node_modules && rm -rf */.next && rm -rf */dist",
    "postinstall": "cd website && npm install"
  },
  "keywords": [
    "open-source",
    "collaboration",
    "software-development",
    "community",
    "nextjs",
    "react",
    "typescript"
  ],
  "author": "OpenLaunch Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PraiseTechzw/OpenLaunch.git"
  },
  "bugs": {
    "url": "https://github.com/PraiseTechzw/OpenLaunch/issues"
  },
  "homepage": "https://github.com/PraiseTechzw/OpenLaunch#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  }
}